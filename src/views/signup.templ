package views

templ SignupPage() {
	@Index("login page") {
		<form action="/signup" method="POST">
			<label> fullname <input type="text" name="fullname" /> </label>
			<label> email <input type="email" name="email" /> </label>
			<label> password <input type="password" name="password" /> </label>
			<button> login </button>
		</form>
	}
}
